        android:label="CategoryListVehicleFragment">
        <action
            android:id="@+id/action_categoryListVehicleFragment_to_categoryDetailVehicleFragment"
            app:destination="@id/categoryDetailVehicleFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/categoryDetailFilmFragment"
        android:name="com.edufelip.challengealpha.presentation.fragments.category_detail.category_detail_film.CategoryDetailFilmFragment"
        android:label="CategoryDetailFilmFragment" >
        <argument
            android:name="film"
            app:argType="com.edufelip.challengealpha.domain.models.Film" />
    </fragment>
    <fragment
        android:id="@+id/categoryDetailPeopleFragment"
        android:name="com.edufelip.challengealpha.presentation.fragments.category_detail.category_detail_people.CategoryDetailPeopleFragment"
        android:label="CategoryDetailPeopleFragment" >
        <argument
            android:name="people"
            app:argType="com.edufelip.challengealpha.domain.models.People" />
    </fragment>
    <fragment
        android:id="@+id/categoryDetailSpecieFragment"
        android:name="com.edufelip.challengealpha.presentation.fragments.category_detail.category_detail_specie.CategoryDetailSpecieFragment"
        android:label="CategoryDetailSpecieFragment" >
        <argument
            android:name="specie"
            app:argType="com.edufelip.challengealpha.domain.models.Specie" />
    </fragment>
    <fragment
        android:id="@+id/categoryDetailStarshipFragment"
        android:name="com.edufelip.challengealpha.presentation.fragments.category_detail.category_detail_starship.CategoryDetailStarshipFragment"
