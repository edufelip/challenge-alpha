            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/categoryListPeopleFragment"
        android:name="com.edufelip.challengealpha.presentation.fragments.category_list.category_list_people.CategoryListPeopleFragment"
        android:label="CategoryListPeopleFragment">
