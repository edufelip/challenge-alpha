            android:id="@+id/action_categoryListPeopleFragment_to_categoryDetailPeopleFragment"
            app:destination="@id/categoryDetailPeopleFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/categoryListPlanetFragment"
