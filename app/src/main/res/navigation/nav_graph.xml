            app:argType="com.edufelip.challengealpha.domain.models.Planet" />
    </fragment>
    <fragment
        android:id="@+id/favoritesFragment"
        android:name="com.edufelip.challengealpha.presentation.fragments.favorites.FavoritesFragment"
        android:label="FavoritesFragment" />
</navigation>